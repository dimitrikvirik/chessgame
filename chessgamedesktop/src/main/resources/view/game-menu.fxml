<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<Pane fx:id="pane" prefHeight="332.0" prefWidth="509.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="git.dimitrikvirik.chessgamedesktop.controller.GameMenuController">
   <Button layoutX="215.0" layoutY="81.0" mnemonicParsing="false" onMouseClicked="#createGame" prefHeight="26.0" prefWidth="100.0" text="Generate Id">
      <cursor>
         <Cursor fx:constant="HAND" />
      </cursor></Button>
   <Button layoutX="386.0" layoutY="154.0" mnemonicParsing="false" onMouseClicked="#joinGame" prefHeight="25.0" prefWidth="46.0" text="Join">
      <cursor>
         <Cursor fx:constant="HAND" />
      </cursor></Button>
   <TextField fx:id="gameId" layoutX="151.0" layoutY="154.0" prefHeight="25.0" prefWidth="220.0" promptText="GameId" />
   <TextField fx:id="filename" layoutX="145.0" layoutY="252.0" prefHeight="25.0" prefWidth="220.0" promptText="Filename" />
   <Button layoutX="381.0" layoutY="252.0" mnemonicParsing="false" onMouseClicked="#watchGame" prefHeight="25.0" prefWidth="70.0" text="Watch">
      <cursor>
         <Cursor fx:constant="HAND" />
      </cursor>
   </Button>
   <Label layoutX="176.0" layoutY="203.0" prefHeight="30.0" prefWidth="158.0" text="Review Game">
      <font>
         <Font name="System Bold" size="20.0" />
      </font>
   </Label>
   <TextField fx:id="username" layoutX="151.0" layoutY="121.0" prefHeight="25.0" prefWidth="220.0" promptText="Username" />
   <TextField fx:id="server" layoutX="145.0" layoutY="35.0" prefHeight="25.0" prefWidth="220.0" promptText="Server Address" text="localhost:8090" />
</Pane>
