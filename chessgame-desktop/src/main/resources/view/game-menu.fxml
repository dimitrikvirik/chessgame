<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<Pane fx:id="pane" prefHeight="309.0" prefWidth="509.0" xmlns="http://javafx.com/javafx/17"
      xmlns:fx="http://javafx.com/fxml/1" fx:controller="git.dimitrikvirik.chessgamedesktop.controller.GameMenuController">
   <Label layoutX="163.0" layoutY="80.0" prefHeight="30.0" prefWidth="200.0" text="Create Game">
      <font>
         <Font name="System Bold" size="20.0" />
      </font>
   </Label>
   <Label layoutX="200.0" layoutY="170.0" prefHeight="30.0" prefWidth="35.0" text="OR">
      <font>
         <Font name="System Bold" size="20.0" />
      </font>
   </Label>
   <Button layoutX="189.0" layoutY="129.0" mnemonicParsing="false" onMouseClicked="#createGame" prefHeight="26.0" prefWidth="80.0" text="Create">
      <cursor>
         <Cursor fx:constant="HAND" />
      </cursor></Button>
   <Button layoutX="356.0" layoutY="220.0" mnemonicParsing="false" onMouseClicked="#joinGame" prefHeight="25.0" prefWidth="46.0" text="Join">
      <cursor>
         <Cursor fx:constant="HAND" />
      </cursor></Button>
   <TextField fx:id="gameId" layoutX="108.0" layoutY="220.0" prefHeight="25.0" prefWidth="220.0" promptText="GameId" />
</Pane>
